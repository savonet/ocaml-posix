(executable
 (name test)
 (libraries posix-uname))

(rule
 (alias citest)
 (package posix-uname)
 (action
  (run %{exe:test.exe})))

(executable
 (name autogenerated_tests)
 (libraries posix-uname unix))

(rule
 (alias citest)
 (package posix-uname)
 (action
  (run %{exe:autogenerated_tests.exe})))
