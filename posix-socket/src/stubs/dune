(env
 (dev
  (flags
   (:standard -warn-error -A))))

(library
 (name posix_socket_stubs)
 (public_name posix-socket.stubs)
 (libraries posix-socket.types ctypes.stubs))

(rule
 (targets posix_socket_generated_types.ml)
 (deps
  (:gen ../generator/gen_types_c_target.exe))
 (action
  (with-stdout-to
   %{targets}
   (run %{gen}))))
